<path>astraf5.ac</path>


<nasal>
  <load>
    var livery_update = aircraft.livery_update.new("Aircraft/Astra-F5/Models/Liveries", 10);
  </load>   
  <unload>
    livery_update.stop();
  </unload>   
</nasal>

<sound>
  <path>Aircraft/Astra-F5/Sounds/followme-sound.xml</path>
</sound> 

<model> 
 <path>Sign/sign.xml</path>
</model>

<animation>
  <type>material</type>
  <object-name>ldoor_1</object-name>
  <object-name>Mesh11_2</object-name>  
  <object-name>door.rl_1</object-name>
  <object-name>door.rr_1</object-name>
  <object-name>Mesh76.004_1</object-name>
  <property-base>sim/model/livery</property-base>
  <texture-prop>texture</texture-prop>
  <texture>followmeoutside.png</texture>  
  <emission>
    <factor-prop>sim/time/sun-angle-rad</factor-prop>
    <red>   0.1 </red>
    <green> 0.1 </green>
    <blue>  0.1 </blue>
  </emission>
</animation>

<!-- STEERINGWHEEL -->

<animation>  
 <type>rotate</type>  
 <object-name>steeringwheel</object-name>
 <property>surface-positions/rudder-pos-norm</property>
 <factor>-400</factor>
 <axis>
  <x1-m>1.413</x1-m>
  <y1-m>-0.40301</y1-m>
  <z1-m>0.78451</z1-m>
  <x2-m>1.64667</x2-m>
  <y2-m>-0.40301</y2-m>
  <z2-m>0.86655</z2-m>
 </axis>
</animation>

<!-- FRONT WHEEL LEFT -->

<animation>
 <type>rotate</type>
 <object-name>Tyre1</object-name>
 <property>surface-positions/rudder-pos-norm</property>
 <factor>-40</factor>
 <center>
  <x-m> 0.77923</x-m>
  <y-m>-0.73086</y-m>
  <z-m> 0.28864</z-m>
 </center>
 <axis>
  <x>0.0</x>
  <y>0.0</y>
  <z>1.0</z>
 </axis>
</animation>

<animation>
 <type>translate</type>
 <object-name>Tyre1</object-name>
 <property>gear/gear[0]/compression-norm</property>
 <interpolation>
  <entry>
   <ind>0.0</ind>	
   <dep>0</dep>
  </entry>
  <entry>	
	<ind>1.00</ind>
	<dep>0.2</dep>
  </entry>  
 </interpolation>
 <axis>
  <x>0</x>	
  <y>0</y>
  <z>1</z>	
 </axis>
</animation>
 
<animation>
 <type>spin</type>
 <object-name>Tyre1</object-name>
 <property>sim/multiplay/generic/float[11]</property>
 <factor>-6</factor>
 <center>
  <x-m> 0.77923</x-m>
  <y-m>-0.73086</y-m>
  <z-m> 0.28864</z-m>
 </center>
 <axis>
  <x>0.0</x>
  <y>1.0</y>
  <z>0.0</z>
